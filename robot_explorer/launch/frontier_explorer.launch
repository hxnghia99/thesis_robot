<launch>

    <!-- frontier_exploration -->
    <node pkg="frontier_exploration" type="explore_client" name="explore_client" output="screen"/>
    <node pkg="frontier_exploration" type="explore_server" name="explore_server" output="screen" >
        <param name="frequency" type="double" value="5.0"/>
        <param name="goal_aliasing" type="double" value="$(arg sensor_range)"/>
        <rosparam file="$(find turtlebot3_navigation)/param/costmap_common_params_$(arg model).yaml" command="load" ns="explore_costmap" />
        <rosparam file="$(find turtlebot3_slam)/config/frontier_exploration.yaml" command="load" ns="explore_costmap" />
    </node>

    <node pkg = "robot_explorer" type = "prometheus" name = "prometheus" output = "screen"/>
    <!--
    <node pkg = "robot_explorer" type = "prometheus" name = "prometheus" output = "screen" launch-prefix="gnome-terminal (-)-command"/>
    -->
</launch>