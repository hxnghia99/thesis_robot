<launch>

  <!-- Arguments -->
  <arg name="move_forward_only" default="true"/>
  <arg name="sensor_range" default="1.0"/>
  <arg name="cmd_vel_topic" default="/cmd_vel" />
  <arg name="odom_topic" default="odom" />
  <arg name="rviz" default="false"/>

  <!--include file="$(find thesis_robot_bringup)/launch/thesis_robot_rtabmap.launch">
      <arg name="rviz" value="$(arg rviz)"/>
  </include-->
  
  <include file="$(find thesis_robot_bringup)/launch/thesis_robot_remote.launch"/>

  <arg name="slam_methods" default="hector"/>
  <!-- SLAM: Gmapping, Cartographer, Hector, Karto, Frontier_exploration, RTAB-Map -->
  <!--include file="$(find robot_slam)/launch/robot_$(arg slam_methods).launch

  <!-- AMCL -->
  <node pkg="timed_roslaunch" type="timed_roslaunch.sh" name="Delay_amcl" args="1 robot_navigation amcl.launch" output="screen"/>

  <!-- move_base -->
  <!--node pkg="timed_roslaunch" type="timed_roslaunch.sh" name="Delay_move_base" args="2 robot_navigation move_base.launch" output="screen"/-->
  
  <!--frontier_explorer-->
  <node pkg="timed_roslaunch" type="timed_roslaunch.sh" name="Delay_frontier_explorer" args="3 robot_explorer frontier_explorer.launch" output="screen"/>

    <!-- Rviz -->
  <node if="$(arg rviz)" name="rviz" pkg="rviz" type="rviz" args="-d $(find robot_explorer)/rviz/robot_prometheus_frontier_explorer.rviz"/>

</launch>
